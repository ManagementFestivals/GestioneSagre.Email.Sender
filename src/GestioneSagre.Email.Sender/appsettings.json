{
    "Kestrel": {
        "Limits": {
            "MaxRequestBodySize": 5242880
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "ConnectionStrings": {
        "Default": "Data Source=NOME-SERVER\\NOME-ISTANZA;Initial Catalog=NOME-DATABASE;User ID=USERNAME;Password=PASSWORD"
    },
    "Smtp": {
        "Host": "example.org",
        "Port": 25,
        "Security": "StartTls",
        "Username": "Username SMTP",
        "Password": "Password SMTP",
        "Sender": "MyName <noreply@example.org>"
    }
}